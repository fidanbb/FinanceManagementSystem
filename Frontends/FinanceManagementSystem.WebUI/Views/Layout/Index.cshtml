
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_HeadLayoutComponentPartial")


<body>

    <!-- Begin page -->
    <div id="layout-wrapper">
        @await Component.InvokeAsync("_NavbarLayoutComponentPartial")


        <!-- ========== Left Sidebar Start ========== -->
        @await Component.InvokeAsync("_SidebarLayoutComponentPartial")

      
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="main-content">

            <div class="page-content">
                <div class="container-fluid">

                    <!-- start page title -->
                    <div class="row">
                        <div class="col-12">
                            @RenderBody()
                        </div>
                    </div>
                    <!-- end page title -->

                   

                </div> <!-- container-fluid -->
            </div>
            <!-- End Page-content -->
            @await Component.InvokeAsync("_FooterLayoutComponentPartial")

           

        </div>
        <!-- end main content-->

    </div>
    <!-- END layout-wrapper -->
    <!-- Overlay-->
    <div class="menu-overlay"></div>

    @await Component.InvokeAsync("_ScriptsLayoutComponentPartial")


   

</body>

</html>
